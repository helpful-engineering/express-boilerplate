{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "express boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "nodemon",
    "swagger": "tsoa spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpful-engineering-co/express-boilerplate.git"
  },
  "keywords": [
    "express",
    "typescript",
    "rest",
    "api"
  ],
  "author": "kylekrny",
  "license": "ISC",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/helpful-engineering-co/express-boilerplate/issues"
  },
  "homepage": "https://github.com/helpful-engineering-co/express-boilerplate#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "swagger-ui-express": "^4.6.0",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
